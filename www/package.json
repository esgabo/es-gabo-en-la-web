    "postcss-import": "^12.0.1",
    "postcss-load-config": "^2.1.0",
    "postcss-nested": "^4.2.1",
    "reading-time": "^1.2.0",
    "rollup": "^2.3.4",
    "rollup-plugin-glob": "^1.0.2",
    "rollup-plugin-svelte": "^5.2.2",
    "rollup-plugin-terser": "^5.3.0",
    "rss": "^1.2.2",
    "sapper": "^0.27.0",
