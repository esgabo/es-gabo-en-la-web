  "license": "",
  "version": "0.0.1",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/ilgespinel/es-gabo-en-la-web.git"
  },
  "scripts": {
    "dev": "run-p start:dev watch:css",
    "build": "npm run build:css && sapper build --legacy",
    "watch:css": "postcss src/tailwind.css -o static/index.css -w",
    "build:css": "cross-env NODE_ENV=production postcss src/tailwind.css -o static/index.css",
    "export": "npm run build:css && sapper export --legacy",
    "start": "node __sapper__/build",
    "start:dev": "sapper dev",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "polka": "next",
    "sirv": "^0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "cross-env": "^7.0.2",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.10",
    "gray-matter": "^4.0.2",
    "markdown-it": "^11.0.0",
    "markdown-it-block-embed": "0.0.3",
